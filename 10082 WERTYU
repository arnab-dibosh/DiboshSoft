//10082 WERTYU
#include<bits/stdc++.h>

int main()
{
      char arr[]="`1234567890-=QWERTYUIOP[]\\ASDFGHJKL;'ZXCVBNM,./";
      char c;

      int l=strlen(arr);
      while(scanf("%c", &c)!=EOF)
      {
	      for(int i=0; i<l; i++)
	        if(c==arr[i]) c=arr[i-1];
	        
          printf("%c", c);
      }

    return 0;
}

